 [
	{
		"description": "这是一个带有headers信息的post请求",
		"request": {
			"uri": "/demoPostWithHeaders",
			"method": "post",
			"headers": {
				"content-type": "application/json"
			},
			"json": {
				"name": "wanglaosi",
				"sex": "women"
			}
		},
		"response": {
			"status": 200,
			"json": {
				"wanglaosi": "success",
				"status": "1"
			}
		}
	}
]